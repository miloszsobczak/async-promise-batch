{
    "parser": "typescript",
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": ["prettier-plugin-sort-imports"],
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "parser": "json",
                "printWidth": 200
            }
        }
    ],
    "semi": true,
    "tabWidth": 4,
    "useTabs": false
}
